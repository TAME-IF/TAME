module
{
	title = "init inheritance test";
}

action general a_test;

object o_base
{
	init()
	{
		x = 6;
	}
}

object o_obj1 : o_base {}
object o_obj2 : o_base { init() {x=2;}}
object o_obj3 : o_obj2 {}
object o_obj4 : o_obj1 {}
object o_obj5 : o_obj3 {}

world
{
	init()
	{
		queueAction(a_test);
	}
	
	onAction(a_test)
	{
		textln(o_obj1.x);
		textln(o_obj2.x);
		textln(o_obj3.x);
		textln(o_obj4.x);
		textln(o_obj5.x);
		quit;
	}
	
}
