module
{
	title = "Call Me Maybe";
}

action general a_call named "call";
action general a_callmaybe named "callmaybe";
action general a_quit named "quit";

object o_test
{
	procedure("butt")
	{
		textln("container butt!!");
	}
	
}

world
{
	onAction(a_quit)
	{
		quit;
	}

	procedure("butt")
	{
		textln("Butt!");
	}
	
	onAction(a_call)
	{
		call("butt");
		callFrom(o_test, "butt");
		call("butt2");
		callFrom(o_test, "butt2");
	}
	
	onAction(a_callmaybe)
	{
		callMaybe("butt");
		callMaybeFrom(o_test, "butt");
		callMaybe("butt2");
		callMaybeFrom(o_test, "butt2");
	}
	
	AfterModuleInit()
	{
		textln("READY!");
	}
	
}

